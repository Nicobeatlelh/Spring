  	<servlet-name>ProductosServlet</servlet-name>
  	<display-name>ProductosServlet</display-name>
  	<description></description>
  	<servlet-class>newProject.servlet.ProductosServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ClientesServlet</servlet-name>
  	<url-pattern>/ClientesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClientesServletBusqueda</servlet-name>
  	<url-pattern>/ClientesServletBusqueda</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductosServlet</servlet-name>
